{
    "name": "Copy to clipboardWrite Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "storage",
      "activeTab",
      "clipboardWrite"
    ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello.png"
    }
  }